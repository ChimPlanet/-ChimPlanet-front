    "baseUrl": ".",
    "target": "es6",
    "paths": {
      "@component/*": [
        "./src/components/*"
      ],
      "@api/*": [
        "./src/api/*"
      ],
      "@atoms/*": [
        "./src/atoms/*"
      ],
      "@constants/*": [
        "./src/constants/*"
